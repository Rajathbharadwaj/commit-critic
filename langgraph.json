{
  "dependencies": ["."],
  "graphs": {
    "commit_critic": "commit_critic.graph:create_commit_critic_graph"
  },
  "env": "./.env",
  "python_version": "3.11",
  "store": {
    "ttl": {
      "refresh_on_read": true,
      "sweep_interval_minutes": 60,
      "default_ttl": 1440
    }
  },
  "checkpointer": {
    "ttl": {
      "strategy": "delete",
      "sweep_interval_minutes": 120,
      "default_ttl": 43200
    }
  },
  "http": {
    "cors": {
      "allow_origins": ["*", "https://smith.langchain.com"],
      "allow_methods": ["*"],
      "allow_headers": ["*"],
      "allow_credentials": true,
      "expose_headers": ["*"]
    },
    "allowed_origins": ["https://smith.langchain.com"]
  }
}
